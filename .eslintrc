{
  "extends": "eslint-config-airbnb",
  "env": {
    "browser": true,
    "node": true,
    "jasmine": true
  },
  "parser": "babel-eslint",
  "rules": {
    "quotes": [2, "single"],
    "comma-dangle": [1, "always-multiline"],
    "no-underscore-dangle": [0],
    "react/jsx-uses-react": 2,
    "react/jsx-uses-vars": 2,
    "react/react-in-jsx-scope": 2
  },
  "plugins": [
    "react"
  ],
  "globals": {
    "__": true
  }
}
